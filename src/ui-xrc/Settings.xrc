<?xml version="1.0" ?>
<resource>
  <object class="wxDialog" name="dlgAlignment" subclass="AlignDlg">
    <title>Alignment</title>
    <centered>1</centered>
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="sizeritem">
        <object class="wxRadioBox" name="rdHorizontal">
          <label>Horizontal</label>
          <content>
            <item>N&amp;o Change</item>
            <item>Align &amp;Left</item>
            <item>Align C&amp;enter</item>
            <item>Align &amp;Right</item>
            <item>Slant Left</item>
            <item>Slant Right</item>
            <item>Scatter</item>
            <item>Jog</item>
            <item>Space E&amp;qually</item>
            <item>&amp;Stack</item>
          </content>
          <style>wxRA_SPECIFY_COLS</style>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxRadioBox" name="rdVertical">
          <label>Vertical</label>
          <content>
            <item>&amp;No Change</item>
            <item>Align &amp;Top</item>
            <item>Align &amp;Center</item>
            <item>Align &amp;Bottom</item>
            <item>Slant Up</item>
            <item>Slant Down</item>
            <item>Scatter</item>
            <item>Jog</item>
            <item>Space Eq&amp;ually</item>
            <item>Stac&amp;k</item>
          </content>
          <style>wxRA_SPECIFY_COLS</style>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxRadioBox" name="rdWidth">
              <label>Width</label>
              <content>
                <item>No Change</item>
                <item>Narrowest</item>
                <item>Average</item>
                <item>Widest</item>
                <item>Increasing</item>
                <item>Decreasing</item>
                <item>Scatter</item>
                <item>Jog</item>
              </content>
              <style>wxRA_SPECIFY_COLS</style>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_OK">
              <label>&amp;Ok</label>
              <default>1</default>
            </object>
            <flag>wxALIGN_CENTRE</flag>
          </object>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxRadioBox" name="rdHeight">
              <label>Height</label>
              <content>
                <item>No Change</item>
                <item>Shortest</item>
                <item>Average</item>
                <item>Tallest</item>
                <item>Increasing</item>
                <item>Decreasing</item>
                <item>Scatter</item>
                <item>Jog</item>
              </content>
              <style>wxRA_SPECIFY_COLS</style>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_CANCEL">
              <label>&amp;Cancel</label>
            </object>
            <flag>wxALIGN_CENTRE</flag>
          </object>
        </object>
      </object>
    </object>
  </object>
  <object class="wxDialog" name="dlgBitmapProps" subclass="BitmapPropertiesDlg">
    <title>Bitmap Properties</title>
    <centered>1</centered>
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxStaticBoxSizer">
              <label>Bitmap Size</label>
              <orient>wxVERTICAL</orient>
              <object class="sizeritem">
                <object class="wxRadioBox">
                  <label></label>
                  <content>
                    <item>320x200</item>
                    <item>640x480</item>
                    <item>800x600</item>
                    <item>1024x768</item>
                    <item>1152x864</item>
                    <item>1280x1024</item>
                    <item>1600x1200</item>
                    <item>Custom</item>
                    <item>Window Size</item>
                  </content>
                  <dimension>2</dimension>
                  <style>wxRA_SPECIFY_COLS|wxNO_BORDER</style>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Custom Window Size</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxHORIZONTAL</orient>
                  <object class="sizeritem">
                    <object class="wxTextCtrl"/>
                    <flag>wxEXPAND|wxGROW</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>x</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl"/>
                    <flag>wxEXPAND|wxGROW</flag>
                  </object>
                </object>
                <flag>wxEXPAND|wxGROW</flag>
              </object>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioBox">
              <label>Scaling</label>
              <content>
                <item>Current Zoom Level</item>
                <item>Show Entire Map</item>
              </content>
              <style>wxRA_SPECIFY_COLS</style>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticBoxSizer">
              <label>JPEG Settings</label>
              <orient>wxVERTICAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Image Quality</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxSlider">
                  <value>75</value>
                  <min></min>
                  <max>100</max>
                  <size>100,15</size>
                  <style>wxSL_HORIZONTAL|wxSL_LABELS</style>
                  <tickfreq>5</tickfreq>
                  <tick>5</tick>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxCheckBox">
                  <label>Image Smoothing</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxCheckBox">
                  <label>Progressive Encoding</label>
                </object>
              </object>
            </object>
          </object>
        </object>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxStaticBitmap">
              <bitmap></bitmap>
              <size>320,320</size>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxButton" name="wxOK">
                  <label>&amp;Ok</label>
                  <default>1</default>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxButton" name="wxCANCEL">
                  <label>&amp;Cancel</label>
                </object>
              </object>
            </object>
            <flag>wxALIGN_CENTRE</flag>
          </object>
        </object>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxDialog" name="dlgChartGrid" subclass="ChartGrid">
    <title>Rosette Chart Grid</title>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
