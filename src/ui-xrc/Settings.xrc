  <object class="wxDialog" name="dlgSettings" subclass="SettingsDlg">
    <title>Settings</title>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxNotebook" name="pnlAllSettings">
          <object class="notebookpage">
            <label>General</label>
            <object class="wxPanel" name="pnlSettingsGeneral">
              <object class="wxBoxSizer">
                <orient>wxVERTICAL</orient>
                <object class="sizeritem">
                  <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Maximum levels of undo</label>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxSpinCtrl" name="pnlSettingsGeneralMaxUndo">
                        <value>20</value>
                        <min>1</min>
                        <max>100</max>
                      </object>
                    </object>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                      <object class="wxCheckBox" name="checkVisualOverlayDisplay">
                        <label>Visual overlay display:</label>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxRadioButton" name="rdbColorCoded">
                        <label>Color Coded</label>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxRadioButton" name="rdbIcons">
                        <label>Icons</label>
                      </object>
                    </object>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox" name="checkMakeBackupFiles">
                    <label>Make backup files</label>
                    <checked>1</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox" name="checkUseDraft">
                    <label>Use draft display when panning and scrolling</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox" name="checkPrintIcons">
                    <label>Print icons as polygons</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox" name="checkSuppressPrintHyperlinks">
                    <label>Suppress output of hyperlinks to printer</label>
                    <checked>1</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox" name="checkAntiAliasBitmaps">
                    <label>Anti-alias bitmaps in the symbol palette</label>
                    <checked>1</checked>
                  </object>
                </object>
              </object>
            </object>
          </object>
          <object class="notebookpage">
            <label>Selection</label>
            <object class="wxPanel" name="pnlSettingsSelection">
              <object class="wxBoxSizer">
                <orient>wxVERTICAL</orient>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>Automatically select new objects</label>
                    <checked>1</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>Right click prompts to close shape</label>
                    <checked>1</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>Freeze properties of selected objects</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>Autogroup complex objects when created</label>
                    <checked>1</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>Crosshair on by default</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Zoom Factor</label>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxTextCtrl">
                        <value>2.0</value>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
          </object>
          <object class="notebookpage">
            <label>Auto-order</label>
            <object class="wxPanel" name="pnlSettingsAutoOrder">
              <object class="wxBoxSizer">
                <orient>wxHORIZONTAL</orient>
                <object class="sizeritem">
                  <object class="wxRadioBox">
                    <label>Auto-Order Setting</label>
                    <content>
                      <item>No Change</item>
                      <item>Top Left</item>
                      <item>Top Right</item>
                      <item>Bottom Left</item>
                      <item>Bottom Right</item>
                      <item>Center</item>
                      <item>Edge</item>
                      <item>Random</item>
                    </content>
                    <style>wxRA_SPECIFY_COLS</style>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Auto-Order allows you to
automatically order multiple symbols
after placement. This can be useful
for symbols designed to overlap in a
certain way. To leave symbols in the
exact order that you added them,
select &quot;No Change&quot;.</label>
                  </object>
                </object>
              </object>
            </object>
          </object>
          <object class="notebookpage">
            <label>Drawing</label>
            <object class="wxPanel" name="pnlSettingsDrawing">
              <object class="wxBoxSizer">
                <orient>wxVERTICAL</orient>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>New text size is relative to zoom level</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>New icon size is relative to zoom level</label>
                    <checked>1</checked>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxCheckBox">
                    <label>New icon size equals the original symbol size</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <object class="wxRadioBox">
                    <label>Freehand Quality</label>
                    <content>
                      <item>Smooth (High Node Density)</item>
                      <item>Standard</item>
                      <item>Selective (Low Node Density)</item>
                    </content>
                    <selection>1</selection>
                    <style>wxRA_SPECIFY_COLS</style>
                  </object>
                </object>
              </object>
            </object>
          </object>
          <usenotebooksizer>1</usenotebooksizer>
        </object>
        <flag>wxEXPAND|wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_OK">
              <label>Ok</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_CANCEL">
              <label>Cancel</label>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
  <object class="wxDialog" name="dlgMapSettings" subclass="MapSettingsDlg">
    <title>Map Settings</title>
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="sizeritem">
