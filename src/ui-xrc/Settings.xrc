                                <label>C=</label>
                              </object>
                            </object>
                            <object class="sizeritem">
                              <object class="wxTextCtrl"/>
                            </object>
                          </object>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
          </object>
          <object class="notebookpage">
